{
    "color": {
        "$type": "color",
        "warning-100": {
            "$value": "#f0c044"
        },
        "warning-50": {
            "$value": "#ffe9bd"
        }
    },
    "border-radius": {
        "$type": "dimension",
        "xs": {
            "$value": "0.02em"
        },
        "sm": {
            "$value": "3px"
        },
        "md": {
            "$value": "5px"
        },
        "lg": {
            "$value": "999px"
        },
        "xl": {
            "$value": "50%"
        }
    },
    "shadow": {
        "$type": "shadow",
        "sm": {
            "$value": "0 1px 3px rgba(12, 12, 12, 0.09)"
        },
        "md": {
            "$value": "0 0 0 1px var(--n-color-border), 0 1px 5px rgba(12, 12, 12, 0.05), 0 0 40px rgba(12, 12, 12, 0.015)"
        },
        "lg": {
            "$value": "0 0 0 1px var(--n-color-border), 0 5px 17px rgba(12, 12, 12, 0.14)"
        }
    },
    "font-size": {
        "$type": "dimension",
        "xxxl": {
            "$value": "2.25rem"
        },
        "xxl": {
            "$value": "1.5rrem"
        },
        "xl": {
            "$value": "1.25rem"
        },
        "lg": {
            "$value": "1rem"
        },
        "md": {
            "$value": "0.875rem"
        },
        "sm": {
            "$value": "0.75rem"
        },
        "xs": {
            "$value": "0.6875rem"
        }
    },
    "font": {},
    "line-height": {
        "$type": "number",
        "tight": {
            "$value": "1.15"
        },
        "normal": {
            "$value": "1.3"
        }
    },
    "size": {},
    "space": {},
    "transition": {},
    "z-index": {},
    "button": {
        "background-color": {
            "$value": "{color.warning-100}",
            "$type": "color",
            "$description": "The background color for buttons in their normal state."
        },
        "font-size": {
            "$value": "{font-size.xxl}",
            "$type": "number"
        }
    }
}
